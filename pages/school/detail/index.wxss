page {
  background: #FFF;
}

.img-box {
  height: 312rpx;
  margin-bottom: 90rpx;
}

.img-box image {
  width: 100%;
  height: 100%;
}
.title-tips {
  margin: 0 55rpx;
}
.title {
  font-size: 30rpx;
  color: #666;
  width: 100%;
  text-align: center;
  margin-top: 45rpx;
}

.desc {
  font-size: 30rpx;
  color: #b1b1b1;
  margin: 10rpx 35rpx;
  text-align: center;
}

.list-custom .weui-media-box__hd_in-appmsg {
  width: 140rpx;
  height: 140rpx;
}

.logo-bg {
  position: absolute;
  width: 104rpx;
  height: 115rpx;
  margin: auto;
  top: 250rpx;
  left: 50%;
  margin-left: -52rpx;
}

.logo-bg image {
  width: 100%;
  height: 100%;
}

.logo-bg .logo-img {
  position: absolute;
  left: 8rpx;
  bottom: 0;
  width: 90rpx;
  height: 90rpx;
}
.time {
  font-size: 24rpx;
  color: #999;
  line-height: 36rpx;
  border-bottom: 2rpx solid #DDD;
  padding-bottom: 20rpx;
  margin: 0 25rpx;
}
.title {
  font-size: 32rpx;
  font-weight: bold;
  text-align: left;
  padding: 25rpx;
  margin-top: 0;
  line-height: 40rpx;
}
.title-block {
  background: #FFF;
  padding-bottom: 35rpx;
  margin-bottom: 10rpx;
}

.list-pic {
  position: relative;
  width: 640rpx;
  height: 440rpx;
  margin: 45rpx auto 0 auto;
  border-radius: 10rpx;
  border: 2rpx solid #DDD;
  overflow: hidden;
  text-align: left;
  background: #FFF;
}
.list-pic .item-title {
  font-size: 30rpx;
  color: #3a3a3a;
  width: 598rpx;
  padding: 0 25rpx;
  line-height: 60rpx;
  height: 60rpx;
  margin-top: 0;
  top: 0;
  text-overflow: ellipsis;
  overflow: hidden;
  display: block;
  white-space: nowrap;
}
.list-pic image {
  width: 100%;
  height: 350rpx;
}